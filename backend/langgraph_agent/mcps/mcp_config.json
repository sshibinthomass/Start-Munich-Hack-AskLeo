{
  "mcpServers": {
    "dataflow": {
      "command": "python",
      "args": ["${MCP_FOLDER_DIR}"],
      "transport": "stdio"
    },
    "filesystem": {
      "command": "npx",
      "args": [
        "-y",
        "@modelcontextprotocol/server-filesystem",
        "${MCP_FILESYSTEM_DIR}"
      ],
      "transport": "stdio"
    },
    "git": {
      "command": "uvx",
      "args": ["mcp-server-git"],
      "transport": "stdio"
    },
    "arxiv-mcp-server-gpt": {
      "command": "npx",
      "args": [
        "-y",
        "@smithery/cli@latest",
        "run",
        "@lecigarevolant/arxiv-mcp-server-gpt",
        "--key",
        "2a2bed04-d517-4c23-8d36-90f3fff30ad8",
        "--profile",
        "external-falcon-AVl9Oq"
      ],
      "transport": "stdio"
    },
    "duckduckgo": {
      "command": "docker",
      "args": ["run", "-i", "--rm", "mcp/duckduckgo"],
      "transport": "stdio"
    },
    "wikipedia-mcp": {
      "command": "docker",
      "args": ["run", "-i", "--rm", "mcp/wikipedia-mcp"],
      "transport": "stdio"
    }
  }
}
